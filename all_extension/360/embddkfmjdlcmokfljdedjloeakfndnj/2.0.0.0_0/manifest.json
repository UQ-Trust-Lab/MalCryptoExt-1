{
   "author": "Bytom frontend, zhitinglin",
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/logo.png",
      "default_popup": "pages/popup.html"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "js/content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Bytom Wallet Chrome extension, Bystore officially changed to Byone.",
   "icons": {
      "128": "icons/logo.png",
      "16": "icons/logo.png",
      "48": "icons/logo.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Byone",
   "permissions": [ "*://*/", "storage", "unlimitedStorage", "contextMenus", "clipboardWrite", "clipboardRead", "background" ],
   "version": "2.0.0.0",
   "web_accessible_resources": [ "js/inject.js", "wasm/main.wasm" ]
}

{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup/popup.html",
      "default_title": "Math Wallet"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "content_start.js" ],
      "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "content_end.js" ],
      "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "The MetaMask Scatter TronLink PolkadotJS Alternative All-in-One Crypto Wallet",
   "homepage_url": "http://www.mathwallet.org",
   "icons": {
      "128": "icons/icon_128.png",
      "16": "icons/icon_16.png",
      "32": "icons/icon_32.png",
      "48": "icons/icon_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Math Wallet",
   "permissions": [ "storage", "unlimitedStorage", "http://*/*", "https://*/*" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "2.1.9",
   "web_accessible_resources": [ "inject.js", "inject_start.js", "inject_end.js" ]
}

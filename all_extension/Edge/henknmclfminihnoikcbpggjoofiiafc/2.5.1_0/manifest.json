{
   "background": {
      "persistent": true,
      "scripts": [ "./js/bg.js" ]
   },
   "browser_action": {
      "default_icon": "img/logos/bitbns-48--white.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "css": [ "./css/styles.css" ],
      "js": [ "./js/binance.js" ],
      "matches": [ "https://*.binance.com/*" ]
   }, {
      "css": [ "./css/styles.css" ],
      "js": [ "./js/gateio.js" ],
      "matches": [ "https://*.gate.io/*" ]
   }, {
      "css": [ "./css/styles.css" ],
      "js": [ "./js/huobi.js" ],
      "matches": [ "https://*.huobi.com/*", "https://*.hbg.com/*" ]
   }, {
      "css": [ "./css/styles.css" ],
      "js": [ "./js/kucoin.js" ],
      "matches": [ "https://*.kucoin.com/*" ]
   } ],
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "current_locale": "10",
   "description": "Explore arbitrage opportunities and trade faster between Bitbns, Binance, Huobi, Gate.io and Kucoin",
   "icons": {
      "128": "img/logos/logo-128--whiteTxt.png",
      "16": "img/logos/logo-16--whiteTxt.png",
      "32": "img/logos/logo-32--whiteTxt.png",
      "48": "img/logos/logo-48--whiteTxt.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Bitbns Extension",
   "permissions": [ "webRequest", "https://bitbns.com/*", "https://api.bitbns.com/*", "https://buyhatke.com/*" ],
   "short_name": "Bitbns",
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "2.5.1",
   "web_accessible_resources": [ "img/*" ]
}

{
   "author": "coredev@waykichainhk.com",
   "background": {
      "page": "pages/background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "128": "icons/128.png",
         "16": "icons/16.png"
      },
      "default_popup": "pages/popup.html",
      "default_title": "WaykiMax"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/inject.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "A WaykiChain Wallet in your Browser",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "WaykiMax",
   "permissions": [ "\u003Call_urls>", "*://*/*", "activeTab", "tabs", "unlimitedStorage", "storage" ],
   "version": "2.2.2",
   "web_accessible_resources": [ "panel.html", "js/content.js" ]
}
